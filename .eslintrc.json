{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            1
        ],
        "prefer-template": [
            0
        ],
        "max-len": [2, 110, 4],
        "comma-dangle": [2, "never"]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
        "requestIdleCallback": false,
        "requireNode": false
    }
}
